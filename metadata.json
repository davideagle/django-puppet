{
    "name"        : "chedi-django",
    "author"      : "chedi",
    "version"     : "0.6.3",
    "source"      : "https://github.com/Chedi/django-puppet",
    "license"     : "GPL-3.0",
    "summary"     : "Deploy a Django app from git into an Nginx/Postgresql/Redis/Gunicorn stack",
    "issues_url"  : "https://github.com/Chedi/django-puppet/issues",
    "project_page": "https://github.com/Chedi/django-puppet",
    "operatingsystem_support": [{
        "operatingsystem"       : "Fedora",
